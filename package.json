{
  "name": "calsot-ci",
  "version": "0.0.7",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "calsot-ci": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "deps:ci": "npm ci",
    "deps:clean": "rm -rf ./node_modules && rm -f ./package-lock.json",
    "deps": "npm run deps:clean && npm install --legacy-peer-deps --no-audit --no-fund",
    "lint": "eslint --ext ts src",
    "prepare": "rm -rf dist && npm run build",
    "test:ci": "vitest --run",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmoralp/calsot-sdk.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rmoralp/calsot-sdk/issues"
  },
  "homepage": "https://github.com/rmoralp/calsot-sdk#readme",
  "devDependencies": {
    "@maons/lint": "0",
    "@maons/tsconfig": "0",
    "typescript": "4.9.5",
    "vitest": "0.29.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/@maons/lint/.eslintrc.js"
  },
  "prettier": "@maons/lint/.prettierrc.js",
  "dependencies": {
    "commander": "10.0.0",
    "datadog-metrics": "0.11.0",
    "execa": "5.1.1",
    "lighthouse": "10.0.1"
  }
}
